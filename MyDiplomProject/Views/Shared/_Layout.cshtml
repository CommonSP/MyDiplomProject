<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title</title>
    <link rel="stylesheet" href="~/Content/site/css/style.css">
   
</head>
<body>
    <nav>
        <div class="navigation">
            <div class="navigation-container">
                <ul class="navigation-items">
                    <li class="navigate-item">
                        @Html.ActionLink("Главная", "Index", "Home", null, new { @class = "navigation-link" })
                    </li>
                    <li class="navigate-item">
                        @Html.ActionLink("Галерея", "Index", "Galery", null, new { @class = "navigation-link" })
                    </li>
                    <li class="navigate-item">
                        @Html.ActionLink("Сообщения", "Index", "Message", null, new { @class = "navigation-link" })
                    </li>
                </ul>
                <ul class="navigation-items">
                    @Html.Action("UserData", "Layout")

                </ul>
            </div>
        </div>
    </nav>
    <div class="content">
        @RenderBody()

    </div>
    @Html.Action("UserProfil", "Layout");
    <script src="~/Content/site/scripts/jquery-3.5.1.min.js"></script>
    <script src="~/Content/site/scripts/js.js"></script>
    <script src="~/Scripts/jquery.unobtrusive-ajax.js"></script>
    <script src="~/Scripts/jquery.signalR-2.4.1.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/axios/dist/axios.min.js"></script>
    <script src="~/signalr/hubs"></script>
</body>
</html >
